<div class="center hero-unit">
  <h3>Places</h3>
  
<% @places.each do |place| %>
<div class="well well-large">
<div class="container-fluid">
  <div class="row-fluid">
    <div class="span3">
      <ul class="thumbnails">
        <li class="span10">
          <div class="thumbnail">
            <img <%= link_to image_tag(place.imagen) %>
          </div>
        </li>
      </ul>
    </div>
    <div class="span8">
      <fieldset>
    <legend><%= place.Name %></legend>
    <label>Address: <%= place.address %></label>
  <small>Phone: <%= place.phone %></small>
    <p>
<%= link_to 'View Details', place %>
    </p>
  </fieldset>
    </div>
  </div>
</div>

 </div>
 <br/>
<% end %>
</div>

